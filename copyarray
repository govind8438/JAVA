public class copyarray {
    public static void main(String[] args) {
        int a[]={2,3,4,6,1,7,9,8};
        int b[]= new int[10];
        for (int i=0; i<a.length; i++)
        {
           b[i]=a[i];
        }
        for (int i=0; i<a.length; i++)
        {
            System.out.print(","+b[i]);
        }
    }
}
